# Annotation grammar

lang: annotate

import "./assignment.grammar"

COLON = /:/
LANG<keyword> = /lang/
ID = /[a-zA-Z_]\w*/
COMMENT ?= /#[^\n]*\n/ { tokenize: { "type": "comment" } }
SPACE ?= /\s+/

annotate -> LANG COLON ID@lang ...assignments;

assignments -> ...assignments? assignment@assignments;